  "bugs": {
    "url": "https://github.com/MrDman1/estimator-RPA/issues"
  },
  "homepage": "https://github.com/MrDman1/estimator-RPA#readme",
  "dependencies": {
    "playwright": "^1.55.1"
  }
}
