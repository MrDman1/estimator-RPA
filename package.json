{
  "name": "estimator-rpa",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && node --test test/*.js",
    "e2e": "npx playwright test",
    "e2e:headed": "npx playwright test --headed",
    "codegen": "npx playwright codegen",
    "install:pw": "npx playwright install --with-deps",
    "e2e:auth:max": "npx playwright test tests/setup/auth.setup.spec.ts -g \"Maximizer auth\"",
    "e2e:auth:nsd": "npx playwright test tests/setup/auth.setup.spec.ts -g \"NSD auth\"",
    "e2e:download": "npx playwright test tests/e2e/download.spec.ts"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "@playwright/test": "^1.55.1"
  },
  "description": "<!--\r   NOTE FOR DEVELOPERS:\r   This repository has been cleaned up and consolidated into a single, self\u2011contained\r   project.  Historical patch files and temporary README fragments have been removed,\r   and all documented fixes have been incorporated directly into the source code.\r   See the \"Developer Notes\" section near the bottom of this file for guidance on\r   extending or modifying the application.\r -->",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDman1/estimator-RPA.git"
  },
  "keywords": [],
