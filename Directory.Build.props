<Project>
  <PropertyGroup>
    <ForcedRoot>C:\dev\NuformBuild\</ForcedRoot>
    <OutputPath>$(ForcedRoot)bin\$(MSBuildProjectName)\$(Configuration)\</OutputPath>
    <IntermediateOutputPath>$(ForcedRoot)obj\$(MSBuildProjectName)\$(Configuration)\</IntermediateOutputPath>
    <AppendTargetFrameworkToOutputPath>true</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
  </PropertyGroup>
  <Target Name="AfterBuild">
    <Exec Command="powershell -ExecutionPolicy Bypass -NoLogo -NoProfile -Command &quot;try { Unblock-File -Path '$(TargetPath)'; Unblock-File -Path '$(TargetDir)\*' -ErrorAction SilentlyContinue } catch {}&quot;" />
  </Target>
</Project>
